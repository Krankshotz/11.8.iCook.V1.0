<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.jordan.icook.MainActivity"
    tools:layout_editor_absoluteY="25dp"
    tools:layout_editor_absoluteX="0dp">

    <ImageView
        android:id="@+id/background"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:cropToPadding="true"
        android:scaleType="centerCrop"
        android:visibility="visible"
        app:srcCompat="@drawable/bcgrdcrop"
        tools:ignore="RtlHardcoded"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintRight_toRightOf="parent" />

    <ImageButton
        android:id="@+id/preferencesButton4"
        android:layout_width="144dp"
        android:layout_height="144dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/btton4"
        android:layout_marginEnd="30dp"
        app:layout_constraintRight_toLeftOf="@+id/guideline"
        android:layout_marginRight="30dp"
        app:layout_constraintTop_toTopOf="@+id/background"
        android:layout_marginTop="332dp"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline4"
        app:layout_constraintHorizontal_bias="0.0" />

    <ImageButton
        android:id="@+id/recipesButton3"
        android:layout_width="144dp"
        android:layout_height="144dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/btton3"
        android:layout_marginStart="28dp"
        android:layout_marginLeft="28dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline2"
        app:layout_constraintTop_toTopOf="@+id/background"
        android:layout_marginTop="332dp"
        app:layout_constraintRight_toLeftOf="@+id/guideline3"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="1.0" />

    <ImageButton
        android:id="@+id/receiptButton1"
        android:layout_width="144dp"
        android:layout_height="144dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"
        android:tint="@android:color/transparent"
        app:srcCompat="@drawable/btton1"
        android:layout_marginStart="28dp"
        app:layout_constraintTop_toTopOf="@+id/background"
        android:layout_marginTop="179dp"
        android:layout_marginLeft="28dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline2"
        app:layout_constraintRight_toLeftOf="@+id/guideline3"
        android:layout_marginRight="8dp"
        app:layout_constraintHorizontal_bias="1.0" />

    <ImageButton
        android:id="@+id/pantryButton2"
        android:layout_width="144dp"
        android:layout_height="144dp"
        android:adjustViewBounds="true"
        android:background="@android:color/transparent"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/btton2"
        tools:ignore="ContentDescription"
        android:layout_marginEnd="30dp"
        app:layout_constraintRight_toLeftOf="@+id/guideline"
        android:layout_marginRight="20dp"
        app:layout_constraintTop_toTopOf="@+id/background"
        android:layout_marginTop="179dp"
        android:layout_marginLeft="0dp"
        app:layout_constraintLeft_toLeftOf="@+id/guideline4"
        app:layout_constraintHorizontal_bias="0.0" />

    <ImageView
        android:id="@+id/banner"
        android:layout_width="0dp"
        android:layout_height="99dp"
        app:srcCompat="@drawable/banner"
        tools:layout_editor_absoluteY="8dp"
        tools:layout_editor_absoluteX="0dp" />

    <ImageButton
        android:id="@+id/Button5Help"
        android:layout_width="49dp"
        android:layout_height="46dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="3dp"
        android:layout_marginRight="8dp"
        android:background="@android:color/transparent"
        android:backgroundTint="@android:color/background_light"
        android:tint="@android:color/background_dark"
        app:layout_constraintBottom_toBottomOf="@+id/background"
        app:layout_constraintRight_toLeftOf="@+id/guideline"
        app:srcCompat="@android:drawable/ic_menu_help" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.96" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline2"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.04" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline3"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.48958334" />

    <android.support.constraint.Guideline
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/guideline4"
        android:orientation="vertical"
        app:layout_constraintGuide_percent="0.5260417" />

</android.support.constraint.ConstraintLayout>
